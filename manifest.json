{
  "name": "Roast Wander Extension",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Wanders the major public rooms sequentially",
  "permissions": [
    "activeTab", "tabs", "https://meet.jit.si/*"
  ],
  "browser_action": {
      "default_title": "wander roast"
  },
  "background": {
      "scripts": ["background.js", "constants.js"],
      "persistent": false
  },
  "web_accessible_resources": [
    "wander.js"
  ]
}
